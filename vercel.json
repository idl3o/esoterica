{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "library-index.json",
          "distillations/**",
          "synthesis/**",
          "protocols/**",
          "seeds/**",
          "traditions/**",
          "translated/**",
          "extractions/**",
          "fiction-bridges/**",
          "constellation/**",
          "client/views/**"
        ]
      }
    },
    {
      "src": "client/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "constellation/**",
      "use": "@vercel/static"
    },
    {
      "src": "distillations/**",
      "use": "@vercel/static"
    },
    {
      "src": "synthesis/**/*.md",
      "use": "@vercel/static"
    },
    {
      "src": "protocols/**/*.md",
      "use": "@vercel/static"
    },
    {
      "src": "seeds/**/*.md",
      "use": "@vercel/static"
    },
    {
      "src": "traditions/**/*.md",
      "use": "@vercel/static"
    },
    {
      "src": "translated/**/*.md",
      "use": "@vercel/static"
    },
    {
      "src": "extractions/**/*.md",
      "use": "@vercel/static"
    },
    {
      "src": "fiction-bridges/**",
      "use": "@vercel/static"
    },
    {
      "src": "synthesis-library.html",
      "use": "@vercel/static"
    },
    {
      "src": "synthesis-index.json",
      "use": "@vercel/static"
    },
    {
      "src": "oracle.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/explorer",
      "dest": "/constellation/constellation_explorer_v2.html"
    },
    {
      "src": "/explorer-3d",
      "dest": "/constellation/constellation_explorer_v3.html"
    },
    {
      "src": "/constellation-explorer",
      "dest": "/constellation/constellation_explorer_v2.html"
    },
    {
      "src": "/synthesis-library",
      "dest": "/synthesis-library.html"
    },
    {
      "src": "/synthesis-index.json",
      "dest": "/synthesis-index.json"
    },
    {
      "src": "/static/(.*)",
      "dest": "/client/public/$1"
    },
    {
      "src": "/constellation/(.*)",
      "dest": "/constellation/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
